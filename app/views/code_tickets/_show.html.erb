<%= render :partial => "blurb", :locals => { :ticket => @ticket }  %>

<% @details.each do |detail| %>
  <%= render :partial => "code_details/blurb", :locals => { :detail => detail }  %>
<% end %>

<% @ticket.code_commits.each do |commit| %>
  <%= render :partial => "code_commits/blurb", :locals => { :commit => commit }  %>
<% end %>
